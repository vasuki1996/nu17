<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vasuki.nu17" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NU17</name>
    <description>
        A Application for Nandi Utsav 2k17
    </description>
    <author email="vasukivardhan96@gmail.com" href="http://xtron.cf">
        The Indian Professional Team    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.1.2" />
    <engine name="windows" spec="~4.4.3" />
    <engine name="wp8" spec="~3.8.2" />
</widget>
